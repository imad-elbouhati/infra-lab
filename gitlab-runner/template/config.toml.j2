[[runners]]
  name = "docker-runner"
  url = "https://gitlab.com/"
  token = "{{ gitlab_runner_token }}"
  executor = "docker"

[runners.docker]
  image = "alpine:latest"
  privileged = false
  disable_entrypoint_overwrite = false
  oom_kill_disable = false
  disable_cache = false
  volumes = [
    "/cache",
  ]
  network_mode = "host"